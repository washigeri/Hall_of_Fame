<div class="col-md2 col-md-offset-5">
<h1>Posts#index</h1>
<body> Liste des posts</body>
	<% @post.each do |a| %>
	<h1><%= a.titre %></h1>
	<p> <%= a.contenu %></p>
    <img src="<%= link_to(image_tag(a.image_url, :alt => "alt text", :class => "anyclass"), a.image_url) %>" class="img-responsive" alt="Responsive image">
	<% end %>
<p>Find me in app/views/posts/index.html.erb</p>
</div>